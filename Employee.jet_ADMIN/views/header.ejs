<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet" />
<!-- Google Fonts -->
<link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
<!-- MDB -->
<link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/4.3.0/mdb.min.css" rel="stylesheet" />
<!-- MDB -->
<link rel="stylesheet" href="/css/header.css">


<header>
    <!-- Sidebar -->
    <nav id="sidebarMenu" class="collapse d-lg-block sidebar collapse bg-white">
        <div class="position-sticky">
            <div class="list-group list-group-flush mx-3 mt-4">
                <a href="/"
                    class="dashboard-sidebar-items list-group-item list-group-item-action py-2 ripple <%= (onPage==='dashboard')?'active':''%>"
                    aria-current="true">
                    <i class="fas fa-tachometer-alt fa-fw me-3"></i><span>Dashboard</span></a>
                <a href="/employees"
                    class="dashboard-sidebar-items list-group-item list-group-item-action py-2 ripple <%= (onPage==='employee')?'active':''%>">
                    <i class="fa-solid fa-users me-3"></i></i><span>Employees</span></a>
                <a href="/feedback"
                    class="dashboard-sidebar-items list-group-item list-group-item-action py-2 ripple <%= (onPage==='feedback')?'active':''%>">
                    <i class="fas fa-solid fa-comment-dots me-3"></i><span>Feedbacks</span></a>
                <a href="/projects"
                    class="dashboard-sidebar-items list-group-item list-group-item-action py-2 ripple <%= (onPage==='projects')?'active':''%>">
                    <i class="fa-solid fa-diagram-project me-3"></i></i><span>Projects</span></a>

                <a href="/requests"
                    class="dashboard-sidebar-items list-group-item list-group-item-action py-2 ripple <%= (onPage==='requests')?'active':''%>">
                    <i class="fas fa-arrow-right-arrow-left fa-fw me-3"></i><span>Requests</span></a>
            </div>
        </div>
    </nav>
    <!-- Sidebar -->

    <!-- Navbar -->
    <nav id="main-navbar" class="navbar navbar-expand-lg navbar-light bg-white fixed-top">
        <!-- Container wrapper -->
        <div class="container-fluid">
            <!-- Toggle button -->
            <button class="navbar-toggler" type="button" data-mdb-toggle="collapse" data-mdb-target="#sidebarMenu"
                aria-controls="sidebarMenu" aria-expanded="false" aria-label="Toggle navigation">
                <i class="fas fa-bars"></i>
            </button>

            <!-- Brand -->
            <a class="navbar-brand" href="#">

                <img class="rounded-circle mt-3" src="https://i.imgur.com/CXQmsmF.png" height="70" alt="MDB Logo"
                    loading="lazy" />
                <p class="mt-4" style="padding:2px">YourCompany</p>
            </a>
            <!-- <img src="" class="logo"> -->


            <!-- Right links -->
            <ul class="navbar-nav ms-auto d-flex flex-row">

                <!-- Avatar -->
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle hidden-arrow d-flex align-items-center" href="#"
                        id="navbarDropdownMenuLink" role="button" data-mdb-toggle="dropdown" aria-expanded="false">
                        <img src="https://mdbcdn.b-cdn.net/img/Photos/Avatars/img (31).webp" class="rounded-circle"
                            height="50" alt="Avatar" loading="lazy" />
                    </a>
                    <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdownMenuLink">
                        <li>
                            <a class="dropdown-item" href="/admin/logout">Logout</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
        <!-- Container wrapper -->
    </nav>
    <!-- Navbar -->



</header>