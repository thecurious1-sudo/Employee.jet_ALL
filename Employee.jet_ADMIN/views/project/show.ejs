<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">
<link rel="stylesheet" href="/css/project/show.css">
<div class="content">
    <div class="container">
        <div class="row">
            <div class="col-lg-9 mt-3 ml-5">
                <div class="card-box task-detail">
                    <div class="media mt-0 m-b-30"><img class="d-flex mr-3 rounded-circle" alt="64x64"
                            src="../<%=project.supervisor.avatar%>" style="width: 72px; height: 72px;">
                        <div class="media-body">
                            <h5 class="media-heading mb-0 mt-2">
                                <%=project.supervisor.name%>
                            </h5><span class="badge badge-danger mt-1">Supervisor</span>
                        </div>
                    </div>
                    <h4 class="m-b-20 mt-4"><%=project.name%></h4>
                    <p class="text-muted">
                        <%=project.description%>
                    </p>
                    <ul class="list-inline task-dates m-b-0 mt-5">
                        <li>
                            <h5 class="m-b-5">Start Date</h5>
                            <p>
                                <%=project.createdAt.toString().substring(0,15)%>
                            </p>
                        </li>
                        <li>
                            <h5 class="m-b-5">End Date</h5>
                            <%if(project.finishedAt){%>
                                <p>
                                    <%=project.finishedAt.toString().substring(0,15)%>
                                </p>
                                <%}else{%>
                                    <p>Currently ongoing</p>
                                    <%}%>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                    <div class="task-tags mt-4">
                        <h5 class="">Tags</h5>
                        <div class="bootstrap-tagsinput mt-3">
                            <% for(let tag of project.tags){ %>
                                    <span class="tag label label-tag"><%=tag%><span data-role="remove"></span></span>
                                    
                                 <%}%>
                        
                        </div>

                    </div>
                    <div class="attached-files mt-4">
                        <h5 class="">Members</h5>
                        <div class="files-list">
                            <%for(let team of project.team){%>
                            <div class="file-box m-2">
                                <a href="/employees/showDetails/<%=team._id%>"><img src="../<%=team.avatar%>"
                                        class="img-responsive img-thumbnail" alt="attached-img"></a>
                                <p class="font-13 mb-1 text-muted ml-2"><small><%=team.name%></small></p>
                            </div>
                            <%}%>
                        </div>
                    </div>
                    
                    <h5 class="">Feedbacks</h5>
                    <a href="feedback/sendFeedback/<%= project._id %>" class="btn btn-primary mt-3">Send Feedback Form To
                        Members</a>
                </div>
                
            </div>
            
